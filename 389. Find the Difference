class Solution {
public:
    char findTheDifference(string s, string t) {
        long int sum=0,sum1=0;
        for(int i=0;i<s.length();i++)
        {
            sum+=int(s[i]);
        }
        for(int i=0;i<t.length();i++)
        {
            sum1+=int(t[i]);
        }
        char ans=char(sum1-sum);
        //cout<<ans;
        return ans;
        
    }
};
